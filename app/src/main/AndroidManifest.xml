<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.traveloiddevs.traveloid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Traveloid">
        <activity
            android:name=".detail.prurba"
            android:exported="false" />
        <activity
            android:name=".register.Prueba"
            android:exported="false" />
        <activity
            android:name=".list.pryebaactivity"
            android:exported="false" />
        <activity
            android:name=".register.RegisterTouristPlaceActivity"
            android:exported="false" />
        <activity
            android:name=".list.ListTraveloidActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".detail.DetailActivity"
            android:exported="true"
            android:screenOrientation="portrait" />
    </application>

</manifest>